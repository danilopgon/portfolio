{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "singularName": "blog-post",
    "pluralName": "blog-posts",
    "displayName": "Blog post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "blog_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::blog-category.blog-category",
      "inversedBy": "blog_posts"
    },
    "description": {
      "type": "text",
      "required": true
    },
    "thumbnail": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    }
  }
}
